GET     /                           controllers.CRUDController.index

GET     /users                      controllers.CRUDController.getAllUsers

GET     /user/:userLogin            controllers.CRUDController.getUser(userLogin: String)

POST    /addUser                    controllers.CRUDController.addUser

PUT     /updateUser                 controllers.CRUDController.updateUser

DELETE  /deleteUser/:userLogin      controllers.CRUDController.deleteUser(userLogin: String)